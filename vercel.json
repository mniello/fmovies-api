{
    "name": "Fmovies-api",
    "version": 2,
    "builds": [
        { "src": "app.py", "use": "@liudonghua123/now-flask" }
    ],
    "routes": [
        { "src": "(.*)", "dest": "app.py" }
    ],
    "output": {
        "api/*": {
            "type": "http",
            "status": 200,
            "contentType": "application/json"
        }
    }
}

